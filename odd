		String num = "123456";
		
		int[] array = new int[num.length()];
		
		String odd = "";
		String even = "";
		
		for(int i = 0 ; i < num.length() ; i++) {
			if((Integer.parseInt(""+num.charAt(i))%2) == 0 )
					even += num.charAt(i);
			else
				odd += num.charAt(i);
			
		}
		
		String oddeven = even+odd ;
		for(int i = 0 ; i < oddeven.length() ; i++) {
			
			array[i] =Integer.parseInt(""+oddeven.charAt(i));
			
		}
		
		for(int e : array)
		System.out.print(e+" ");
		
		
